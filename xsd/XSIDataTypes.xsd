    <!-- This file defines the common and elementary schema types and elements    -->
    <!-- (xs:complexType and xs:simpleType and xs:element)  required by the XML   -->
    <!-- Schema files in the http://schema.broadsoft.com/xsi name space           -->
    <!-- The Elements and Types are defined in the Alphabetical Order with        -->
    <!-- grouping at the Element, Complex and Simple level respectively.          -->
    <!-- ************************************************************************ -->

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://schema.broadsoft.com/xsi"
  targetNamespace="http://schema.broadsoft.com/xsi"
  attributeFormDefault="unqualified" elementFormDefault="qualified"
  version="1.0">

  <!-- **************************************************************-->
  <!--         Common Elements Used Across XSI name space            -->
  <!--                    in Alphabetical Order.                     -->
  <!-- **************************************************************-->

  <xs:element name="CallMeNowStartInfo">
    <xs:annotation>
      <xs:documentation>
        Provides information in the response to an accepted Call Me Now request.

        Release Added: Xsi-Actions 8.5 
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
	<xs:element name="passcode" type="CallMeNowPasscode" minOccurs="0">
          <xs:annotation>
            <xs:documentation>
              The passcode to use for the Call Me Now call.  Only present 
              when the Call Me Now service answer confirmation option set to Passcode.
            </xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
    </xs:complexType>            
  </xs:element>

  <xs:element name="CallStartInfo">
    <xs:annotation>
      <xs:documentation>
        Identifies a new Call with CallId and External
        Tracking Id.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="callId" type="CallId" />
        <xs:element name="externalTrackingId" type="ExternalTrackingId" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="QueueCallOrder">
    <xs:annotation>
      <xs:documentation>
        Determines the position of queue entry in the
        Call Center's queue.
        Note that while reordering a call in
        queue,
        new position may not always match the
        requested position due to
        the dynamic nature of the queue.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="position" type="PositiveInt" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>


  <!-- **************************************************************-->
  <!--        Common Complex Type Used Across XSI namespace.        -->
  <!--                    in Alphabetical Order.                     -->
  <!-- **************************************************************-->


  <xs:complexType name="AccessEndpoint">
    <xs:annotation>
      <xs:documentation>
        Contains the details related to an endpoint for
        an access device. An access device is a device 
        that a user can access the AS with 
        (e.g. originate a call a trigger their originating
        services) using an address of record. Note that this
        type does not apply to services such as 
        BroadWorks Anywhere and Remote Office which use a
        network device, not an access device.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="Endpoint">
        <xs:sequence>
          <xs:element name="addressOfRecord" type="AddressOfRecord">
            <xs:annotation>
              <xs:documentation>
                In standalone mode, the address of
                record identifies a
                Line/Port for a device profile. In
                IMS mode, the address of record
                identifies a public identity for an
                identity profile.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:element name="AccessDevice" type="AccessDevice" />
  <xs:complexType name="AccessDevice"> 
    <xs:annotation>
      <xs:documentation>
        Device associated with a user.
        Change History:
        R18.0 - Added
        R19.0 - Added version.
        R20.0 - deviceTypeUrl, deviceUserNamePassword are made optional.
        R21.0 - deviceUserNamePassword is included in Xsi responses only when 
                BroadWorks is configured to allow retrieval of device credentials.
        R21.sp1 - Added elements allowTermination and endpointType. 
                  The endpointType is only present in the GET response when the 
                  client XSI version in the GET request is 21.sp1 or greater.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
     <xs:element name="deviceName" type="AccessDeviceName">
        <xs:annotation>
          <xs:documentation>
               Access device name.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="deviceLevel" type="AccessDeviceLevel">
        <xs:annotation>
          <xs:documentation>
            Identifies at which level in the system an identity/device profile is created.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="deviceType" type="AccessDeviceType">
        <xs:annotation>
          <xs:documentation>
            Access device type.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="deviceLinePort" type="AccessDeviceEndpointLinePort">
        <xs:annotation>
          <xs:documentation>
            The Line/Port identifies a device endpoint 
            in standalone mode or a SIPURI public identity in IMS mode.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="deviceTypeUrl" type="URL" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            Access device type.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="deviceUserNamePassword" type="DeviceManagementUserNamePassword" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            Access device credentials. Included in Xsi responses only when
            BroadWorks is configured to allow retrieval of device credentials.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="version" type="UserAgentSipHeader" minOccurs="0" >
        <xs:annotation>
          <xs:documentation>
            User agent type for the device.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="allowTermination" type="xs:boolean" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            Indicates if the device endpoint location is enabled for terminations.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="endpointType" type="EndpointType" minOccurs="0"> 
        <xs:annotation>
          <xs:documentation>
            Indicates the endpoint type.  
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  
  <xs:complexType name="ACDCallInfo">
    <xs:annotation>
      <xs:documentation>
        An ACDCallInformation is provided when a call is
        offered to an agent
        by an ACD.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="acdUserId" type="UserId">
        <xs:annotation>
          <xs:documentation>
            The subscriber Id of the ACD being used to
            reach the agent
            (the local user).
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="acdName" type="NonEmptyString">
        <xs:annotation>
          <xs:documentation>
            The name configured against the ACD.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="acdNumber" type="Address">
        <xs:annotation>
          <xs:documentation>
            The address of the ACD being used to reach
            the agent. This could be
            the primary DN or a DNIS
            configured
            against the ACD.
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="numCallsInQueue" type="NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
            The number of calls remaining in the queue
            at the time the call is
            offered to the agent.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="waitTime" type="NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
            The amount of time (in seconds) that the
            caller has spent in a
            queue waiting for a agent, at the time
            the call is offered to the
            agent.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="longestWaitTime" type="NonNegativeInt"
        minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The longest wait time (in seconds) of the
            calls remaining in
            the queue, at the time the call is offered
            to the agent.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="holdReminder" type="EmptyContent"
        minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            When present, indicates that an ACD call has
            been held for
            longer than the configured time
            threshold. A
            client application may use this
            information to display highly
            visible indicator to remind the agent to
            retrieve the call.
            It also
            indicates that the call may bounce back to the
            queue
            if not retrieved before
            the configured time threshold.
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="callingPartyInfo" type="PartyInformation">
        <xs:annotation>
          <xs:documentation>
            The information of the calling party.
            Contains the real
            information for the calling party, unlike
            the Call type's
            remoteParty element
            which may be modified for
            call center
            display purposes such
            as by prepending the call
            center name
            to the party's name or
            replacing the party's
            address
            with a DNIS address for the
            call center.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="routePoint" type="EmptyContent"
        minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            Indicates whether the incoming call is being
            offered from a
            route point. This field may be used to
            distinguish the origin
            of the
            incoming call, i.e. call center
            or route point.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="dispositionCode" type="AgentDispositionCode"
        minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            When present, indicates the default
            disposition code
            applicable for this call.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="postCallAgentState" type="NonEmptyString"
        minOccurs="0">
        <xs:annotation>
          <xs:documentation>
          Present only when it's a call center call released 
          by the agent and the option to "Automatically set agent state to 'x' 
          after call" is enabled.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  
  <xs:complexType name="ACDOutgoingCallInfo">
    <xs:annotation>
      <xs:documentation>
        Contains the details of a ACD outgoing call.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="acdUserId" type="UserId">
        <xs:annotation>
          <xs:documentation>
            The user ID of the ACD that the agent is
            making the call as.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="acdName" type="NonEmptyString">
        <xs:annotation>
          <xs:documentation>
            The name configured against the ACD.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="acdNumber" type="Address">
        <xs:annotation>
          <xs:documentation>
            The address of the ACD being used for the
            outgoing call.
            This could be the primary DN or a DNIS
            configured against the ACD.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="routePoint" type="EmptyContent"
        minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            Indicates whether the outgoing call is being
            made as a route point.
            This field may be used to distinguish
            outgoing calls made as ACD with
            the outgoing calls made as a route point.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="dispositionCode" type="AgentDispositionCode"
        minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            When present, indicates the default
            disposition code
            applicable for this call.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="postCallAgentState" type="NonEmptyString"
        minOccurs="0">
        <xs:annotation>
          <xs:documentation>
          Present only when it's a call center call released 
          by the agent and the option to "Automatically set agent state to 'x' 
          after call" is enabled.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  
  <xs:simpleType name="ACDRouting">
    <xs:annotation>
      <xs:documentation>
      Policy for distributing calls to agents which
      belong to Premium call centers.
      Change History:
      R19.0 - Added
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="Priority Based"></xs:enumeration>
      <xs:enumeration value="Skill Based"></xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="ActivatableServiceInfo">
    <xs:annotation>
      <xs:documentation>
        Data that indicates whether a service is active.
        This can be extended
        by any activatable service.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="active" type="xs:boolean"
        minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            When present, indicates the service is
            enabled. When not present, service is inactive.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>


  <xs:complexType name="Address">
    <xs:annotation>
      <xs:documentation>
        Address information as either a SIP or tel URI.
        A SIP URI starts with
        sip:. See RFC 3261 for the
        complete SIP URI
        specification. Note that the URI may
        contain parameters. The
        following are a few examples of SIP URIs that are considered
        valid for XSI:

        sip:+12403649000@broadsoft.com;user=phone
        sip:2403649001@broadsoft.com;user=phone
        sip:9002@broadsoft.com;user=phone
        sip:2403649003;phone-context=+1@broadsoft.com;user=phone
        sip:2403649004;phone-context=as.com@broadsoft.com;user=phone
        sip:user@broadsoft.com

        A
        tel URI starts with tel:. See RFC 3966
        for the complete tel URI
        specification. Note
        that the
        URI may
        contain parameters. The following are a few examples of tel
        URIs
        that are
        considered valid for XSI:

        tel:+12403649000 tel:2403649001
        tel:9002
        tel:2403649003;phone-context=+1
        tel:2403649004;phone-context=as.com

        Note that
        BroadWorks is not strict with RFC 3966 local numbers and allows them without a
        phone-context. Also note that a SIP URI without user=phone present is always treated
        as a URI, not a phone number. Also note that BroadWorks requires that a SIP URI have
        a user portion when the SIP URI is being used as a dialed address (e.g. address for
        a Click-to-Dial or a transfer attempt).
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="NonEmptyURI">
        <xs:attribute name="countryCode" type="CountryCode"
          use="optional">
          <xs:annotation>
            <xs:documentation>
              The country code for the address. Only
              present if the
              address is an E.164 phone number. Note
              that this attribute is only for
              outgoing messages (messages sent
              by the AS). If present for an incoming
              message (message received by the AS),
              then it is ignored.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="NationalPrefix">
    <xs:annotation>
      <xs:documentation>National Prefix.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="PositiveInt" />
  </xs:simpleType>
    
  <xs:complexType name="AudioFileDetails">
    <xs:annotation>
      <xs:documentation>
        Audio File Details.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="fileDescriptor" type="FileDescriptor"/>
    </xs:sequence>
  </xs:complexType>
  
  <xs:complexType name="BroadWorksAnywhereEndpoint">
    <xs:annotation>
      <xs:documentation>
        Contains the details related to a BroadWorks
        Anywhere endpoint.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="NetworkEndpoint" />
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="CallForwarding">
    <xs:annotation>
      <xs:documentation>
        Contains common data for all Call Forwarding services.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ActivatableServiceInfo">
        <xs:sequence>
          <xs:element name="forwardToPhoneNumber" type="Address"
            nillable="true" minOccurs="0">
            <xs:annotation>
              <xs:documentation>
                The forwarded to destination of a Call Forwarding
                service. The field must be present when the service is enabled. It
                may not be present when the service is disabled.
                </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  
  <xs:simpleType name="ClientSessionInfo">
    <xs:annotation>
      <xs:documentation>
        ClientSessionInfo is an opaque token used by a client
        to correlate sessions. 
        
        Change History:
        20     - Added         
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="NonEmptyToken" />
  </xs:simpleType>  
  
  <xs:complexType name="ContactURL">
    <xs:annotation>
      <xs:documentation>
        Contact encapsulates a full Locational
        information in terms of a
        URI and transport like Http or Https.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="uri" type="NonEmptyToken">
        <xs:annotation>
          <xs:documentation>
            This must be given in one the following formats:
            [http/https]://[server]:[port]/[context]/...
            [server]:[port]/context/...
            [server]/context/...
            If the transport is not specified, http:// is used.
            If the port is not specified, the transport's default port is used.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Criteria">
    <xs:annotation>
      <xs:documentation>
        Criteria for services.
                
        Change History:
        R19.0 - Removed criteriaFromDn element. If criteriaFromDn element is required
                use the CriteriaWithFromDn type instead.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="criteriaName" type="CriteriaName"
        minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The must be provided when a new Criteria is
            to be created.
            This should be provided to change the existing
            criteria name
            while modifying a existing Criteria details.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="timeSchedule" type="TimeSchedule"
        minOccurs="0" nillable="true" />
      <xs:element name="holidaySchedule" type="HolidaySchedule"
        minOccurs="0" nillable="true" />
    </xs:sequence>
  </xs:complexType>
  
   <xs:complexType name="CriteriaWithFromDn">
    <xs:annotation>
      <xs:documentation>
        Criteria for services which define criteria for incoming phone number.
        
        Change History:
        R19.0 - Added
      </xs:documentation>
    </xs:annotation>
     <xs:complexContent>
      <xs:extension base="Criteria">
        <xs:sequence>
          <xs:element name="criteriaFromDn" type="CriteriaFromDn" minOccurs="0" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

   <xs:complexType name="CriteriaWithFromDnAndCallToNumber">
    <xs:annotation>
      <xs:documentation>
        Criteria for services which define criteria for incoming
        calling number table and incoming called number table.
        
        Change History:
        R21.0 - Added
      </xs:documentation>
    </xs:annotation>
     <xs:complexContent>
      <xs:extension base="CriteriaWithFromDn">
        <xs:sequence>
          <xs:element name="criteriaCallToNumber" type="CallToNumberList" minOccurs="0" nillable="true" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  
  
  
  <xs:complexType name="CriteriaWithToDn">
    <xs:annotation>
      <xs:documentation>
        Criteria for services which define criteria for outgoing phone number.
        
        Change History:
        R19.0 - Added
      </xs:documentation>
    </xs:annotation>
         <xs:complexContent>
      <xs:extension base="Criteria">
        <xs:sequence>
          <xs:element name="criteriaToDn" type="CriteriaToDn" minOccurs="0" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  
  <xs:complexType name="CriteriaActivation">
    <xs:annotation>
      <xs:documentation>
        Defines the name and the active/inactive status
        of a criteria.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="criteriaName" type="NonEmptyToken">
        <xs:annotation>
          <xs:documentation>
            Indicates the name for the criteria.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="active" type="xs:boolean" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            When present, indicates the criteria is
            active.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="uri" type="URL" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The URI for accessing the other details of
            this Criteria.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="CriteriaActivationList">
    <xs:annotation>
      <xs:documentation>
        Contains a list of criteria.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="criteriaActivation" type="CriteriaActivation"
        minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  
  <xs:complexType name="CriteriaFromDn">
    <xs:annotation>
      <xs:documentation>
        The from dn criteria.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="fromDnCriteriaSelection" type="CriteriaFromDnSelection"
        default="Any" minOccurs="0" />
      <xs:element name="includeAnonymousCallers" type="xs:boolean"
        minOccurs="0" />
      <xs:element name="includeUnavailableCallers" type="xs:boolean"
        minOccurs="0" />
      <xs:element name="phoneNumberList" type="CriteriaPhoneNumberList"
        nillable="true" minOccurs="0" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="CriteriaPhoneNumberList">
    <xs:annotation>
      <xs:documentation>
        A list of phone numbers in Criteria(DNs)
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="phoneNumber" type="DN" minOccurs="1"
        maxOccurs="12" />
    </xs:sequence>
  </xs:complexType>
  
  <xs:simpleType name="CallMeNowAnswerConfirmation">
    <xs:annotation>
      <xs:documentation>
        Possible type of answer confirmation for Call Me Now.
        
        Change History:
        R19.0 - Added
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="None"/>
      <xs:enumeration value="Any Key"/>
      <xs:enumeration value="Passcode"/>
    </xs:restriction>
  </xs:simpleType>
  
  
  <xs:complexType name="CriteriaToDn">
    <xs:annotation>
      <xs:documentation>
        The to dn criteria.
        
        Change History:
        R19.0 - Added
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="toDnCriteriaSelection" type="CriteriaDnSelection"/>
      <xs:element name="phoneNumberList" type="CriteriaPhoneNumberList" nillable="true" minOccurs="0" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="DepartmentKey">
    <xs:annotation>
      <xs:documentation>
        Uniquely identifies a department defined within
        a group or enterprise. To uniquely identify a
        group department, we must
        know the department name and which group and
        subscriberId contains the
        department. To uniquely identify an enterprise
        department, we must know the
        department name and which enterprise
        contains the department.
   </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="serviceProviderId" type="ServiceProviderId" />
      <xs:element name="groupId" type="GroupId" />
      <xs:element name="name" type="DepartmentName" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="DeviceManagementUserNamePassword">
