    <!-- This file defines the schema types and elements required by the            -->
    <!-- BroadWorksAnywhere Events interface in the http://schema.broadsoft.com/xsi -->
    <!-- namespace.                                                                 -->
    <!--                                                                            -->
    <!-- The Elements and Types are defined in the Alphabetical Order with          -->
    <!-- groupind at the Element, Complex and Simple level respectively.            -->
    <!-- ************************************************************************** -->


<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://schema.broadsoft.com/xsi"
    targetNamespace="http://schema.broadsoft.com/xsi" attributeFormDefault="unqualified"
    elementFormDefault="qualified" version="1.0">



    <xs:include schemaLocation="../../XSIBaseEvent.xsd" />

    <xs:include schemaLocation="../XSIServices.xsd" />



    <!-- ******************************************************************** -->
    <!--       B W  A N Y W H E R E  N O T I F I C A T I O N  T Y P E S       -->
    <!--                                                                      -->
    <!-- Types for all XSI BroadWorks Anywhere service related                -->
    <!-- notifications.                                                       -->
    <!-- ******************************************************************** -->

    <xs:complexType name="BroadWorksAnywhereEvent">
        <xs:annotation>
            <xs:documentation>
                Notification with the current configuration status for the BroadWorks
                Anywhere service.        
        
      </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="ServiceEvent">
                <xs:sequence>
                    <xs:element name="info" type="BroadWorksAnywhereInfo" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                Notification data for the BroadWorks Anywhere service.
                                Only not present for service unassignment
                                notifications.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

</xs:schema>