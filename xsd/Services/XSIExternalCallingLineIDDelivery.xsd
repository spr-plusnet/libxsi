<!-- This file defines the schema types and elements required by the          -->
<!-- External Calling Line ID Delivery Service interface in the               -->
<!-- http://schema.broadsoft.com/xsi namespace.                               -->
<!-- ************************************************************************ -->


<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://schema.broadsoft.com/xsi"
  targetNamespace="http://schema.broadsoft.com/xsi"
  attributeFormDefault="unqualified" elementFormDefault="qualified"
  version="1.0">

  <xs:include schemaLocation="../XSIDataTypes.xsd" />
  
  <xs:element name="ExternalCallingLineIDDelivery">
    <xs:annotation>
      <xs:documentation>
        User External Calling Line ID Delivery properties. External Calling Line
        ID Delivery allows the
        Calling Line name and number for callers from outside your group to be
        displayed.
   </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="ActivatableServiceInfo">
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>   
  
</xs:schema>