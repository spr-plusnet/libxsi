  <xs:simpleType name="NumberOfRingsZeroToTwentyExcludeOne">
    <xs:annotation>
      <xs:documentation>
        A CompletionReason indicates the reason the
        play-collect operation has ended.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:int">
      <xs:enumeration value="0" />
      <xs:enumeration value="2" />
      <xs:enumeration value="3" />
      <xs:enumeration value="4" />
      <xs:enumeration value="5" />
      <xs:enumeration value="6" />
      <xs:enumeration value="7" />
      <xs:enumeration value="8" />
      <xs:enumeration value="9" />
      <xs:enumeration value="10" />
      <xs:enumeration value="11" />
      <xs:enumeration value="12" />
      <xs:enumeration value="13" />
      <xs:enumeration value="14" />
      <xs:enumeration value="15" />
      <xs:enumeration value="16" />
      <xs:enumeration value="17" />
      <xs:enumeration value="18" />
      <xs:enumeration value="19" />
      <xs:enumeration value="20" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="OutgoingDN">
    <xs:annotation>
      <xs:documentation>
        An outgoing phone number or a number meant to be
        dialed. It is longer
        than a DN so that equal access digits or
        access code digits may be
        be included. It cannot be a SIP URL.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1" />
      <xs:maxLength value="30" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="OutgoingDNorSIPURI">
    <xs:annotation>
      <xs:documentation>
        Phone Number or SIP URI that can be used to dial.
        This is used to identify addressable entities
        such as a number of URI
        specifically for service control
        elements. URI Validation: - must be
        of
        the format string@string where string is at
        least one valid character
        and there is one and only
        one @. - don't allow sip: - allow
        the following characters in the user
        portions: alphanumeric -
        _ . ! ~
        * ' ( ) - allow
        the following characters in the host portion:
        alphanumeric - .
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1" />
      <xs:maxLength value="161" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="OverflowReason">
    <xs:annotation>
      <xs:documentation>
        An OverflowReason indicates the cause for the
        overflow out of
        RoutePoint or ACD Queue
            </xs:documentation>
    </xs:annotation>
    <xs:restriction base="NonEmptyToken">
      <xs:enumeration value="size">
        <xs:annotation>
          <xs:documentation>
            The call was overflowed because the queue
            had reached its
            maximum capacity when trying to add the
            call.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="time">
        <xs:annotation>
          <xs:documentation>
            The call was overflowed because the call was
            not handled
            within the configured amount of time.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
    
  <xs:simpleType name="Password">
    <xs:restriction base="xs:token">
      <xs:minLength value="1" />
      <xs:maxLength value="60" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="Personality">
    <xs:annotation>
      <xs:documentation>
        The personality of a call.
            </xs:documentation>
    </xs:annotation>
    <xs:restriction base="NonEmptyToken">
      <xs:enumeration value="Originator">
        <xs:annotation>
          <xs:documentation>
            Personality of the call when the subscriber
            is the initiator
            of the call session. If A initiates a call
            session to destination B, then the
            call associated to A has
            its personality
            set to "Originator".
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Terminator">
        <xs:annotation>
          <xs:documentation>
            Personality of the call when the subscriber
            receives an incoming call. If A initiates a 
            call to destination B, then the call
            associated to B has its
            personality set to "Terminator".
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Click-to-Dial">
        <xs:annotation>
          <xs:documentation>
            Personality of the call when a "dial"
            request is being sent on
            behalf of a subscriber. If a remote
            application tries to establish a call
            session from A to B (through a third
            party call control dial request) then
            the call associated to A has its
            personality set to "Click-to-Dial" until A
            answers. Once A has answered, then a
            call attempt is made by BroadWorks to B
            and A's personality is set to
            "Originator".
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="PathHeader">
    <xs:annotation>
      <xs:documentation>
        Provides the routing information to the device associated to the SIP Contact.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1"/>
      <xs:maxLength value="1024"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="PhoneListEntryName">
    <xs:annotation>
      <xs:documentation>
        The name in a phone list entry.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1" />
      <xs:maxLength value="50" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="PositiveInt">
    <xs:annotation>
      <xs:documentation>
        Int that only allows for any positive int value
        (i >= 1).
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:int">
      <xs:minInclusive value="1" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="Port">
    <xs:annotation>
      <xs:documentation>
        TCP/IP Port.
        Change History:
        R18.0 - Added
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:int">
      <xs:minInclusive value="1"/>
      <xs:maxInclusive value="65535"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="RecallType">
    <xs:annotation>
      <xs:documentation>
        The recall type identifies the type of recall or
        callback in effect.
        Release Modified: 20.0
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="NonEmptyToken">
      <xs:enumeration value="Busy Camp On Callback">
        <xs:annotation>
          <xs:documentation>
            The callback was performed because the
            camp-on call remained
            unanswered beyond the configurable
            period.
	  </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Call Transfer Recall">
        <xs:annotation>
          <xs:documentation>
            The callback was performed because the
            transferred call failed
            to reach the destination.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Call Park Recall">
        <xs:annotation>
          <xs:documentation>
            The callback was performed because the
            parked call was not
            retrieved within the configurable
            period.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Automatic Hold/Retrieve Recall">
        <xs:annotation>
          <xs:documentation>
            The callback was performed because the held
            call was not
            retrieved within the configurable
            period.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Executive-Assistant Call Push Recall">
        <xs:annotation>
          <xs:documentation>
            The callback was performed because the call initiated by 
            the executive assistant was not answered by the executive.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="RecordingControls">
    <xs:annotation>
      <xs:documentation>
        The list of the currently allowed recording controls.
        Change History:
        R20.0 - Added
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="NonEmptyToken">
      <xs:enumeration value="none" />
      <xs:enumeration value="record" />
      <xs:enumeration value="pause" />
      <xs:enumeration value="pause-stop" />
      <xs:enumeration value="resume" />
      <xs:enumeration value="resume-stop" />
      <xs:enumeration value="stop" />      
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="RecordingState">
    <xs:annotation>
      <xs:documentation>
        The current state of the call recording.  
        Change History:
        R20.0 - Added
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="NonEmptyToken">
      <xs:enumeration value="Pending">
        <xs:annotation>
          <xs:documentation>
            Call recording has been requested but has not yet been started.  
            This can happen when the recording request is received while a call is held.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Started">
        <xs:annotation>
          <xs:documentation>
            The call is being recorded.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>      
      <xs:enumeration value="Paused">
        <xs:annotation>
          <xs:documentation>
            Call recording was started, and is now paused.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>      
      <xs:enumeration value="Failed">
        <xs:annotation>
          <xs:documentation>
            Call recording was started, but has failed.  In this state, a new call recording cannot be started.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>      
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="RecordingStoppedReason">
    <xs:annotation>
      <xs:documentation>
        The reason why the CallRecordingStoppedEvent is sent.  
        Change History:
        R20.0 - Added
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="NonEmptyToken">
      <xs:enumeration value="User Requested">
        <xs:annotation>
          <xs:documentation>
            The recording was stopped explicitly by the user.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>      
      <xs:enumeration value="Failure">
        <xs:annotation>
          <xs:documentation>
            The recording was stopped due to a failure.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>      
    </xs:restriction>
  </xs:simpleType>   

  <xs:simpleType name="RedirectReason">
    <xs:annotation>
      <xs:documentation>
        The reason for the redirection when a call is
        redirected. Note that the values are defined in
        lowercase just as they would appear in SIP messages.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="NonEmptyToken">
      <xs:enumeration value="user-busy">
        <xs:annotation>
          <xs:documentation>
            The call session was redirected to a
            specified destination
            because Call Forward Busy service is
            activated and the subscriber is busy. A
            subscriber is considered busy when
            there are too many active calls.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="no-answer">
        <xs:annotation>
          <xs:documentation>
            The call session was redirected because Call
            Forward No Answer service was activated and 
            the subscriber did not answer within the 
            Specified number of rings.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="unavailable">
        <xs:annotation>
          <xs:documentation>
            The call session was redirected because Call
            Forwarding Not
            Reachable service was activated and
            the subscriber device is unreachable or
            unregistered.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="unconditional">
        <xs:annotation>
          <xs:documentation>
            The call session was redirected because Call
            Forward Always (CFA) service was activated. 
            When active, CFA causes all incoming calls to 
            be redirected unconditionally.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="time-of-day">
        <xs:annotation>
          <xs:documentation>
            The call session was redirected because Call
            Forward Selective (CFS) was activated and the
            incoming call matched the pre-specified
            time-of-day criteria.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="do-not-disturb">
        <xs:annotation>
          <xs:documentation>
            The call session was redirected because Call
            Forward Busy is enabled and the Do-Not-Disturb
            service was activated.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="deflection">
        <xs:annotation>
          <xs:documentation>
            The call session was redirected following a
            blind transfer
            before the call was answered.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="follow-me">
        <xs:annotation>
          <xs:documentation>
            The call session was redirected because one
            of the following
            services was activated: Simultaneous
            Ringing, Sequential Ringing, BroadWorks
            Anywhere or Remote Office.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="out-of-service">
        <xs:annotation>
          <xs:documentation>
            The call session was redirected by the
            network with the reason
            set to "out-of-service?".
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="away">
        <xs:annotation>
          <xs:documentation>
            The call session was redirected by the
            network with the reason
            set to "away".
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="transfer">
        <xs:annotation>
          <xs:documentation>
            The call session was redirected following a
            blind transfer  after the call was answered.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="voicemail">
        <xs:annotation>
          <xs:documentation>
            The call session was redirected because Call
            Forward Always to
            Voice Mail was activated, or the
            Call Forward Busy to Voice Mail was
            activated and the subscriber
            was busy,
            or the Call Forwarding No Answer to
            Voice Mail was activated and there was
            no answer.
           </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="hunt-group">
        <xs:annotation>
          <xs:documentation>
            The call session was redirected by a
            Hunt-group to an agent.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="call-center">
        <xs:annotation>
          <xs:documentation>
            The call session was redirected by a
            Call-Center to an agent.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="route-point">
        <xs:annotation>
          <xs:documentation>
            The call session was redirected by a
            Route-Point to an agent.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="agent-wrap-up">
        <xs:annotation>
          <xs:documentation>
            The call session was redirected by an
            agent to a wrap-up destination.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="voicexml-script-termination">
        <xs:annotation>
          <xs:documentation>
            The call session was redirected upon
            termination of VoiceXML script.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="unknown">
        <xs:annotation>
          <xs:documentation>
            The call session was redirected for unknown
            reason.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="ReleasingParty">
    <xs:annotation>
      <xs:documentation>
        A ReleasingParty identifies the party that
        releases the call.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="NonEmptyToken">
      <xs:enumeration value="localRelease">
        <xs:annotation>
          <xs:documentation>
            Specify that the release was performed by
            the subscriber
            associated with the call.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="remoteRelease">
        <xs:annotation>
          <xs:documentation>
            Specify that the release was performed by
            the remote party.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="CallingLineIdFirstName">
    <xs:annotation>
      <xs:documentation>
        First Name for Calling Line Id Display.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1" />
      <xs:maxLength value="30" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="CallingLineIdLastName">
    <xs:annotation>
      <xs:documentation>
        Last Name for Calling Line Id Display.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1" />
      <xs:maxLength value="30" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="ScheduleLevel">
    <xs:annotation>
      <xs:documentation>
        Schedule access level.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="Group"></xs:enumeration>
      <xs:enumeration value="User"></xs:enumeration>
      <xs:enumeration value="Enterprise"></xs:enumeration> 
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="ScheduleName">
    <xs:annotation>
      <xs:documentation>
        Time schedule name.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1" />
      <xs:maxLength value="40" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="ScheduleType">
    <xs:annotation>
      <xs:documentation>
        Schedule type.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="Holiday"></xs:enumeration>
      <xs:enumeration value="Time"></xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="ServiceInstanceName">
    <xs:annotation>
      <xs:documentation>
        Service Instance Name for a service without a user id.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1" />
      <xs:maxLength value="80" />
    </xs:restriction>
  </xs:simpleType>
    
  <xs:simpleType name="ServiceInstanceProfileName">
    <xs:annotation>
      <xs:documentation>
        Service Instance Name for a service with a user
        id.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1" />
      <xs:maxLength value="30" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="ServiceProviderId">
    <xs:annotation>
      <xs:documentation>
        Service Provider Id uniquely identifies a
        service provider.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1" />
      <xs:maxLength value="30" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="SimpleThresholdBasedSeverity">
    <xs:annotation>
      <xs:documentation>
         A measurement can be assigned a severity based
         on assigned threshold values. "0" indicates "normal".
         All larger values indicate successively higher (worse) 
         deviations from normal.
         
         Change History:
         19     - Added 
  
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:int">
      <xs:enumeration value="0" />
      <xs:enumeration value="1" />
      <xs:enumeration value="2" />
    </xs:restriction>
  </xs:simpleType>
  

  <xs:simpleType name="SIPURI">
    <xs:annotation>
      <xs:documentation>
        SIP URI.
        The SIP URI is used in many different
        places in the schema.
        If the SIPURI is an alias, the Validation
        rules
        are:
        - don't allow sip:
        - allow the following characters:
        alphanumeric
        - _ . ! ~ * ' ( ) @
        - exactly one @ symbol
        - user portion and host
        portion are both required
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1" />
      <xs:maxLength value="161" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="SkillLevel">
    <xs:annotation>
      <xs:documentation>
        Skill level of the agent in the queue if 
        the queue uses Skill-Based Routing.
        Change History:
        R19.0 - Added
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="PositiveInt" />
  </xs:simpleType>
  

  <xs:simpleType name="SpeedDialDescription">
    <xs:annotation>
      <xs:documentation>Speed dial 8 entry description.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1" />
      <xs:maxLength value="80" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="SubscriberId">
    <xs:annotation>
      <xs:documentation>
        The subscriber Id can either be of the form
        "user@domain" (where domain
        must be included) or it can
        be an E.164 phone number.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="NonEmptyToken" />
  </xs:simpleType>

  <xs:simpleType name="TimeScheduleName">
    <xs:annotation>
      <xs:documentation>
        Time schedule name.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1"/>
      <xs:maxLength value="40"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="Timestamp">
    <xs:annotation>
      <xs:documentation>
        The value of a timestamp is the number of
        milliseconds since January
        1, 1970, 00:00:00 GMT.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:long" />
  </xs:simpleType>

  <xs:simpleType name="TimeZone">
    <xs:annotation>
      <xs:documentation>Time zone key.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1" />
      <xs:maxLength value="127" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="TimeZoneDisplayName">
    <xs:annotation>
      <xs:documentation>
        Localized Time Zone Display Name
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1" />
      <xs:maxLength value="127" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="Title">
    <xs:annotation>
      <xs:documentation>Title.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1" />
      <xs:maxLength value="50" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="TreatmentCompletionReason">
    <xs:annotation>
      <xs:documentation>
        A  TreatmentCompletionReason indicates the cause for the
        treatment completion for a RoutePoint or ACD Queue policy event.
        
        Change History:
        17.sp3 - Added      
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="NonEmptyToken">
      <xs:enumeration value="EOF">
        <xs:annotation>
          <xs:documentation>
            The treatment was completed as expected. 
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="remoteRelease">
        <xs:annotation>
          <xs:documentation>
            The treatment was completed because the call was
            released by the remote party.            
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="URL">
    <xs:annotation>
      <xs:documentation>URL.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1" />
      <xs:maxLength value="256" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="UserAgentSipHeader">
    <xs:annotation>
      <xs:documentation>
        This is the value captured from the User-Agent header that arrives in the user REGISTER request. Please refer to RFC3261 for allowable characters in the User-Agent header.

        Change History:
        R19.0 - Added.
