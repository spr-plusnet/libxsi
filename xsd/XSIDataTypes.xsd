  </xs:complexType> 
  
  <xs:complexType name="VoiceMessagingAlternateNoAnswerGreeting">
    <xs:annotation>
      <xs:documentation>
        The configuration of a alternate no answer greeting.
        It is used when getting a user's voice messaging greeting.
        
        Change History:
        R19.0 - Added.        
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
       <xs:element name="name" type="NetAddress"/>
       <xs:element name="audioFile" type="LabeledMediaFileResource" minOccurs="0"/>
       <xs:element name="videoFile" type="LabeledMediaFileResource" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <!-- ****************************************************************-->
  <!--        Common Simple Type Used Across XSI namespace.           -->
  <!--                    in Alphabetical Order.                       -->
  <!-- ****************************************************************-->

  <xs:simpleType name="AccessDeviceEndpointLinePort">
    <xs:annotation>
      <xs:documentation>
        Also known as address of record, the Line/Port
        identifies a device endpoint
        in standalone mode or a SIPURI
        public identity in IMS mode.
        Line/port user@host or just the port.
        Validation:
        - don't allow sip:
        - allow a leading +
        - allow the
        following characters:
        alphanumeric - _ . ! ~ * ' ( ) @
        - lineports
        for sip devices configured with Proxy Addressing must have a host
        portion
        - lineports for sip devices configured with Device
        Addressing must not have a host portion
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1" />
      <xs:maxLength value="161" />
    </xs:restriction>
  </xs:simpleType>
  
   <xs:simpleType name="AccessDeviceLevel">
    <xs:annotation>
      <xs:documentation>
        Identifies at which level in the system an identity/device profile is created.
        Change History:
        R18.0 - Added
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="System"/>
      <xs:enumeration value="Service Provider"/>
      <xs:enumeration value="Group"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="AccessDeviceName">
    <xs:annotation>
      <xs:documentation>
        Access device name.
        Change History:
        R18.0 - Added
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1"/>
      <xs:maxLength value="40"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="AccessDeviceType">
    <xs:annotation>
      <xs:documentation>
        Access device type.
        Change History:
        R18.0 - Added
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1"/>
      <xs:maxLength value="40"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="AccountAuthorizationCode">
    <xs:annotation>
      <xs:documentation>
        Account/Authorization Code.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="2" />
      <xs:maxLength value="14" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="AccountAuthorizationCodeDescription">
    <xs:annotation>
      <xs:documentation>
        Account/Authorization Code Description.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1" />
      <xs:maxLength value="80" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="ACDConsecutiveBouncedCallsToForceAgentUnavailable">
    <xs:annotation>
      <xs:documentation>
        Call center maximum number of consecutive bounced calls allowed before forcing agent to become unavailable.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:int">
      <xs:minInclusive value="1"/>
      <xs:maxInclusive value="5"/>
    </xs:restriction>
  </xs:simpleType>
      
  <xs:simpleType name="ACDGuardTimerSeconds">
    <xs:annotation>
      <xs:documentation>
         The timer determines how long the system will wait before routing a call to a free agent.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:int">
      <xs:minInclusive value="1"/>
      <xs:maxInclusive value="25"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="AddressLine">
    <xs:annotation>
      <xs:documentation>Street Address of a User.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1" />
      <xs:maxLength value="80" />
    </xs:restriction>
  </xs:simpleType>


  <xs:simpleType name="AddressOfRecord">
    <xs:annotation>
      <xs:documentation>
        In standalone mode, the address of record
        identifies a Line/Port for a
        device profile. In IMS
        mode, the
        address of record identifies a public identity for
        an identity profile.
        Note that when the address of record is a URI, the
        scheme of the URI is not included
        (e.g. it contains user@domain
        instead of sip:user@domain for a
        SIP URI address of record).
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="NonEmptyToken" />
  </xs:simpleType>

  <xs:simpleType name="AgentDispositionCode">
    <xs:annotation>
      <xs:documentation>
        Value of the call center dispostion code (e.g.
        "1", "Code 1").
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="NonEmptyString">
      <xs:maxLength value="10" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="AgentEscalation">
    <xs:annotation>
      <xs:documentation>
        Agent escalation type.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="NonEmptyToken">
      <xs:enumeration value="Normal" />
      <xs:enumeration value="Emergency" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="AgentState">
    <xs:annotation>
      <xs:documentation>
        An AgentState specifies the state of an agent.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="NonEmptyToken">
      <xs:enumeration value="Sign-In">
        <xs:annotation>
          <xs:documentation>
            Agent is signing in. This state is a
            temporary state while logging-in.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Available">
        <xs:annotation>
          <xs:documentation>
            Agent is available.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Unavailable">
        <xs:annotation>
          <xs:documentation>
            Agent is unavailable.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Wrap-Up">
        <xs:annotation>
          <xs:documentation>
            Agent is performing post-call work.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Sign-Out">
        <xs:annotation>
          <xs:documentation>
            Agent is signed out.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>


  <xs:simpleType name="AlternateAddressTypeEnum">
    <xs:annotation>
      <xs:documentation>
        The various token enumerations for the
        AlternateAddressType.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="NonEmptyToken">
      <xs:enumeration value="Fax">
        <xs:annotation>
          <xs:documentation>
            Fax Messaging address.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="DNIS">
        <xs:annotation>
          <xs:documentation>
            DNIS address.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="AlternateAddressType">
    <xs:annotation>
      <xs:documentation>
        Identifies the alternate type for an address.
        The types within the
        union contain all the currently expected
        values.
      </xs:documentation>
    </xs:annotation>
    <xs:union memberTypes="AlternateNumbersIndex AlternateAddressTypeEnum" />
  </xs:simpleType>

  <xs:simpleType name="AlternateNumbersIndex">
    <xs:annotation>
      <xs:documentation>
        Identifies the Alternate Numbers service address
        index.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="PositiveInt">
      <xs:maxInclusive value="10" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="AlternateNumbersRingPatternEnum">
    <xs:annotation>
      <xs:documentation>
        The various token enumerations for the
        AlternateNumbersRingPatternEnum.
     </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="Long-Long" />
      <xs:enumeration value="Short-Short-Long" />
      <xs:enumeration value="Short-Long-Short" />
      <xs:enumeration value="Normal" />
    </xs:restriction>
  </xs:simpleType>
 
  <xs:simpleType name="AnnouncementSelection">
    <xs:annotation>
      <xs:documentation>
        Announcement Selection.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="Default" />
      <xs:enumeration value="Personal" />
    </xs:restriction>
  </xs:simpleType> 
  
  <xs:simpleType name="Appearance">
    <xs:annotation>
      <xs:documentation>
        The appearance value for a call/conference. This
        represents the
        appearance value that BroadWorks
        has assigned to
        the call/conference. It often, but
        not always, maps to the
        appearance value on the device for the call/conference.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="PositiveInt" />
  </xs:simpleType>

  <xs:simpleType name="AudioFileCodec">
    <xs:annotation>
      <xs:documentation>
        Audio file codec.
      </xs:documentation>
    </xs:annotation>
    <xs:union memberTypes="NonEmptyToken AudioFileCodecEnum" />
  </xs:simpleType>
  
  <xs:simpleType name="AudioFileCodecEnum">
    <xs:annotation>
      <xs:documentation>
        Audio file codec.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="None" />
      <xs:enumeration value="G711" />
      <xs:enumeration value="G729" />
      <xs:enumeration value="G726" />
      <xs:enumeration value="AMR" />
      <xs:enumeration value="G722" />
      <xs:enumeration value="AMR-WB" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="AutomaticHoldRetrieveRecallTimerSeconds">
    <xs:annotation>
      <xs:documentation>
        The recall portion of the AHR feature kicks in
        after a call has been
        held for the value
        specified here.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:int">
      <xs:minInclusive value="6" />
      <xs:maxInclusive value="600" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="BusyPattern">
    <xs:annotation>
      <xs:documentation>
        A BusyPattern specifies a busy signal.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="NonEmptyToken">
      <xs:enumeration value="Normal">
        <xs:annotation>
          <xs:documentation>
            Corresponds to the localized busy signal.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="FastBusy">
        <xs:annotation>
          <xs:documentation>
            Corresponds to the localized fast busy
            signal.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="AgentUnavailableCode">
    <xs:annotation>
      <xs:documentation>
        Call Center Agent Unavailable Code Value.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1" />
      <xs:maxLength value="10" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="CallId">
    <xs:annotation>
      <xs:documentation>
        A call ID for a call. A call ID is of the form
        sessionId:callId where
        sessionId is the session identifier and
        callId is the call
        identifier of the call within the session. The
        call ID tracks a
        specific call leg within a call, there will be a
        different call
        ID for the originating vs. terminating half of a
        call. The call
        ID also changes when a call is placed via the Xsi,
        the initial
        call placed will have one value, and when the call
        actually
        rings out to the destination, the ID value will change.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="NonEmptyToken" />
  </xs:simpleType>


  <xs:simpleType name="CallLogId">
    <xs:annotation>
      <xs:documentation>
        A call log ID identifies a call log.
        </xs:documentation>
    </xs:annotation>
    <xs:restriction base="NonEmptyToken">
      <xs:minLength value="1" />
      <xs:maxLength value="30" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="CallMeNowPasscode">
    <xs:annotation>
      <xs:documentation>
        The passcode used by the Call Me Now service.  It is composed 
        of digits (0-9) and is 2 to 30 digits long per system configuration. 
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9]{2,30}"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="MeetMeConferenceUniqueIdentifier">
    <xs:annotation>
      <xs:documentation>
      A list of MeetMe Conference Participant.
        Release Added: R17.sp3
        Release Modified: R18.0
         Occurs when a participant has keyed a self Identifying pin while joining the conference.. 
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9]{1,10}"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="CallType">
    <xs:annotation>
      <xs:documentation>
        Identifies the relationship between the local
        user and another party.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="NonEmptyToken">
      <xs:enumeration value="Group">
        <xs:annotation>
          <xs:documentation>
            The call session is between parties
            belonging to the same
            BroadWorks group. Call Session
            topology:Intra group/enterprise.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Enterprise">
        <xs:annotation>
          <xs:documentation>
            The call session is between parties
            belonging to the same
            BroadWorks Enterprise. Call Session
            topology:Intra group/enterprise.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Network">
        <xs:annotation>
          <xs:documentation>
            One of the party connected to the call
            session is not a
            BroadWorks subscriber. Call Session
            topology:Extra-BroadWorks and
            Intra-BroadWorks topologies.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Emergency">
        <xs:annotation>
          <xs:documentation>
            The call session is connected to an
            emergency party (e.g.
            911). Call Session
            topology:Extra-BroadWorks topology.
         </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Repair">
        <xs:annotation>
          <xs:documentation>
            The call session is connected to a repair
            party. Call Session
            topology:Extra-BroadWorks topology.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="City-Wide Centrex">
        <xs:annotation>
          <xs:documentation>
            The call session was established by using
            the Private Dial
            Plan configured in BroadWorks for a
            City Wide
            Centrex subscriber. Call
            Session topology:Extra-BroadWorks
            topology.
	  </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Private Dial Plan">
        <xs:annotation>
          <xs:documentation>
            The call session was established by using
            the Private Dial
            Plan configured in BroadWorks. Call
            Session
            topology:Extra-BroadWorks
            topology.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Unknown">
        <xs:annotation>
          <xs:documentation>
            The call type is unknown. Call Session
            topology:Extra-BroadWorks and Single call topologies.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Error">
        <xs:annotation>
          <xs:documentation>
            There was an error in determining the call
            type. Call Session
            topology:Extra-BroadWorks topology.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>


  <xs:simpleType name="CallUpdateReason">
    <xs:annotation>
      <xs:documentation>
        The reason for the update when a 
        RoutePointCallUpdated or ACDCallUpdated 
        event is sent.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="NonEmptyToken">
      <xs:enumeration value="Call Transferred">
        <xs:annotation>
          <xs:documentation>
            The call was transferred by the remote 
            party.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Call Picked-Up">
        <xs:annotation>
          <xs:documentation>
            The call was picked-up by the remote 
            party as a result of a Directed Call 
            Pickup request or Directed Call Pickup 
            with Barge-in where the call state was 
            in Alerting.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Call Barged-In">
        <xs:annotation>
          <xs:documentation>
            The call was barged-in as a result of a 
            Directed Call Pickup with Barge-in request.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Call Monitored">
        <xs:annotation>
          <xs:documentation>
            The call is being monitored as a result 
            of a Monitor Call or Monitor Next Call request.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Call Forwarded">
        <xs:annotation>
          <xs:documentation>
            The call is being forwarded as a result 
            of the application of a BroadWorks service 
            (e.g. Call Forward Always).
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Call Park Retrieved">
        <xs:annotation>
          <xs:documentation>
            The call was parked and is being retrieved.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Unknown">
        <xs:annotation>
          <xs:documentation>
            The remote party information is being
            updated, and there is no indication as to 
            the specific reason.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>


  <xs:simpleType name="CDRTerminationCause">
    <xs:annotation>
      <xs:documentation>
        A CdrTerminationCause specifies the completion
        reason of call release
        by the accounting
        interface.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="NonEmptyToken">
      <xs:minLength value="1" />
      <xs:maxLength value="3" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="ChannelTerminatedReason">
    <xs:annotation>
      <xs:documentation>
        A ChannelTerminatedReason indicates the cause for 
        channel termination in a Channel Terminated Event.
        
        Change History:
        19.0 - Added      
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="NonEmptyToken">
      <xs:enumeration value="DuplicateChannelSet">
        <xs:annotation>
          <xs:documentation>
            BroadWorks limits the creation of a single notification 
            channel set for a user per remote application.
            A remote application receiving DuplicateChannelSet reason 
            should terminate the current session and inform the user 
            the reason for termination.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="UserDeleted">
        <xs:annotation>
          <xs:documentation>
            The user for whom the channel was created was deleted.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="NonResponsiveChannel">
        <xs:annotation>
          <xs:documentation>
            All channels in a channel set are not responding to 
            notification events.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>            
      <xs:enumeration value="ExplicitDelete">
        <xs:annotation>
          <xs:documentation>
            The channel was explicitly deleted either by the remote 
            application or by a BroadWorks administrative action.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Expired">
        <xs:annotation>
          <xs:documentation>
            The channel duration expired.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="UnknownChannel">
        <xs:annotation>
          <xs:documentation>
            A notification event included a channel ID that was unknown 
            to the remote application.  The remote application replied 
            with an Unknown Channel (404) response.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>                            
    </xs:restriction>
  </xs:simpleType>  
  

  <xs:simpleType name="CountryCode">
    <xs:annotation>
      <xs:documentation>Country dialing code.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="PositiveInt" />
  </xs:simpleType>


  <xs:simpleType name="CriteriaFromDnSelection">
    <xs:annotation>
      <xs:documentation>
        Selection criteria for a from dn.
        Modified: Rel 20.0
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="Any" />
      <xs:enumeration value="Specified Only" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="CriteriaDnSelection">
    <xs:annotation>
      <xs:documentation>
        Selection criteria for a dn
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="Any" />
      <xs:enumeration value="Specified Only" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="CriteriaName">
    <xs:annotation>
      <xs:documentation>Criteria Name</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1" />
      <xs:maxLength value="50" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="DepartmentName">
    <xs:annotation>
      <xs:documentation>
        Department name. This is only the name of the
        department itself, not the full
        path name of the department and
        all its parents.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1" />
      <xs:maxLength value="50" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="DistinctiveRing">
    <xs:annotation>
      <xs:documentation>
        The distinctive ring pattern that is in use.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="NonEmptyToken">
      <xs:enumeration value="Bellcore-dr2">
        <xs:annotation>
          <xs:documentation>
            Distinctive ringing pattern 2 as defined in
            GR-506-CORE. This
            is used by the Priority Alert service, and
            for the Long-Long ring pattern of
            the Alternate Numbers
            service.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Bellcore-dr3">
        <xs:annotation>
          <xs:documentation>
            Distinctive ringing pattern 3 as defined in
            GR-506-CORE. This
            is used for the Short-Short-Long ring
            pattern of the Alternate Numbers
            service.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Bellcore-dr4">
        <xs:annotation>
          <xs:documentation>
            Distinctive ringing pattern 4 as defined in
            GR-506-CORE. This
            is used for the Short-Long-Short ring
            pattern of the Alternate Numbers
            service.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="DN">
    <xs:annotation>
      <xs:documentation>
        A Directory Number (aka a Phone Number) in e164
        format. This is used to identify addressable
        entities such as a
        number of URI specifically
        for service control elements.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1" />
      <xs:maxLength value="23" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="DNISName">
