              <xs:documentation>
                The external tracking ID for the call.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="extTrackingId" type="ExternalTrackingId">
            <xs:annotation>
              <xs:documentation>
                The external tracking ID for the call.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="routePointName" type="NonEmptyString">
