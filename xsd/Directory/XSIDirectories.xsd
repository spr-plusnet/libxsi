  <!-- This file defines the schema types and elements required by the            -->
  <!-- Directory interface in the http://schema.broadsoft.com/xsi name space.     -->
  <!--                                                                            -->
  <!-- The Elements and Types are defined in the Alphabetical Order with          -->
  <!-- grouping at the Element, Complex and Simple level respectively.            -->
  <!-- ************************************************************************** -->


<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://schema.broadsoft.com/xsi"
  targetNamespace="http://schema.broadsoft.com/xsi" attributeFormDefault="unqualified"
  elementFormDefault="qualified" version="1.0">

  <xs:include schemaLocation="../XSIDataTypes.xsd" />

  <xs:element name="AccessDevices">
    <xs:annotation>
      <xs:documentation>
        List of Access devices associated with a user.
        Change History:
        R18.0 - Added
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="accessDevice" type="AccessDevice" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
    
  <xs:element name="AvailableFlexibleSeatingHosts">
    <xs:annotation>
      <xs:documentation>
          This service allows the retrieval of the list of available flexible seating 
          host stations for a user to use with the flexible seating guest service. 
          
          A flexible seating host can allow a flexible seating guest to be associated the 
          host's device, but using the guest's service profile.

          Change History:
          R20.0 - Added
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="hostUserIdList">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="userID" type="UserId" minOccurs="0" maxOccurs="unbounded" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  
  <xs:element name="AvailableHotelingHosts">
    <xs:annotation>
      <xs:documentation>
          This service allows the retrieval of the list of available hoteling 
          host stations for a user to use with the hoteling guest service.
          
          A hoteling host can allow a hoteling guest to be associated the 
          host's device, but using the guest's service profile.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="hostUserIdList">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="userID" type="UserId" minOccurs="0" maxOccurs="unbounded" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  
  <xs:element name="BroadWorksAnywherePortal">
    <xs:annotation>
      <xs:documentation>
        This command is used to retrieve the BroadWorks Anywhere Portal 
        numbers for a user.  These are the numbers that can be used to 
        initiate a BroadWorks call from a mobile phone using BroadWorks 
        Anywhere.
   </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="phoneNumbers">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="phoneNumber" type="DN" maxOccurs="unbounded" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  
  <xs:element name="CallCenters">
    <xs:annotation>
      <xs:documentation>
        This command is used to retrieve the list of call centers that a user 
        is associated with as an agent or as a supervisor.
        
        Change History:
        17.sp3 - numberCallsInQueue element added. 
                 numberCallsInQueue and serviceUserID regroup under callCenter 
                 element.        
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
      <xs:element name="callCenter" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="serviceUserID" type="UserId">
                <xs:annotation>
                  <xs:documentation>
                    The userid of the Call Center virtual subscriber.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="numberCallsInQueue" type="NonNegativeInt" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>
                    This element is present only when the user is set to 
                    "Agent" and for which the user is the last staffing agent.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>              
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="CallCenterAgents">
    <xs:annotation>
      <xs:documentation>
        This command is used by a Call Center supervisor user to retrieve the 
        list of Call Center Agents being supervised.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="callCenter" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="serviceUserID" type="UserId">
                <xs:annotation>
                  <xs:documentation>
                    The userid of the Call Center virtual subscriber.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="agentList" type="UserDetailInfoList" maxOccurs="unbounded">
                <xs:annotation>
                  <xs:documentation>
                    The list of supervised Call Center Agents belonging to the
                    Call Center. 
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
    

  <xs:element name="CallCenterReportingServer">
    <xs:annotation>
      <xs:documentation> 
        This service provides the list of Call Center Reporting Servers.
        
        Change History
        R19- reportingServer element removed.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
    </xs:complexType>
  </xs:element>

  
  <xs:element name="Supervisors">
    <xs:annotation>
      <xs:documentation>
        This service provides a list of a user’s supervisors for route point 
        and all call centers with which a user is associated.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="callCenters" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="callCenter" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="serviceUserID" type="UserId">
                      <xs:annotation>
                        <xs:documentation>
                          The userid of the Call Center virtual subscriber.
                        </xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="supervisorList" type="UserDetailInfoList"
                      maxOccurs="unbounded" >
                      <xs:annotation>
                        <xs:documentation>
                          The list of Call Center Supervisors belonging to the
                          Call Center. 
                        </xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="routePoint" minOccurs="0">
          <xs:complexType>
            <xs:sequence>              
              <xs:element name="supervisorList" type="UserInfoList" maxOccurs="unbounded">
                <xs:annotation>
                  <xs:documentation>
                    The list of Call Center Supervisors belonging to the
                    Call Center. 
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="CallLogs">
    <xs:annotation>
      <xs:documentation>
        User Call Log directory listing. Call Logs return the most
        recently received, missed, or placed calls for a user.
     </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="placed" type="CallLogsEntries" minOccurs="0" />
        <xs:element name="received" type="CallLogsEntries" minOccurs="0" />
        <xs:element name="missed" type="CallLogsEntries" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  

  <xs:element name="CustomContact">
    <xs:annotation>
      <xs:documentation>
        This Service will provide the list of Directories for a user.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="directory" type="DirectoryDetails" minOccurs="0"
          maxOccurs="unbounded" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  
  <xs:complexType name="DirectoryDetails">
    <xs:annotation>
      <xs:documentation>
        This is the Agent Profile for a particular directory entry. 
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="name" type="CustomContactDirectoryName" minOccurs="0" />
    </xs:sequence>
  </xs:complexType>
  
  <xs:element name="Directory">
    <xs:annotation>
      <xs:documentation>
				List of contacts associated with custom contact directory in a group.        
        Change History:
        R20.0 - Added startIndex, numberOfRecords, and totalAvailableRecords
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
       <xs:element name="startIndex" type="PositiveInt">
          <xs:annotation>
            <xs:documentation>
              The startIndex attribute refers to the position/pointer from 
              which the results will be given.
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="numberOfRecords" type="NonNegativeInt">
          <xs:annotation>
            <xs:documentation>
              The numberOfRecords attribute refers to the number of results 
              returned in the response. The maximum value of number of 
              records will not be greater than the page size.
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="totalAvailableRecords" type="NonNegativeInt">
          <xs:annotation>
            <xs:documentation>
              The totalAvailableRecords attribute refers to the total number
              of responses found for the query.
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="contacts" minOccurs="0">
          <xs:complexType>
            <xs:annotation>
              <xs:documentation>
                This is the Agent Profile for a Particular Directory
              </xs:documentation>
            </xs:annotation>
            <xs:sequence>
              <xs:element name="contact" type="ContactDetails" minOccurs="0"
                maxOccurs="unbounded" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="EnhancedCallLogs">
    <xs:annotation>
      <xs:documentation>
        User Enhanced Call Log directory listing. Call Logs return the most 
        recently received, missed, or placed calls for a user.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="startIndex" type="PositiveInt" />
        <xs:element name="numberOfRecords" type="NonNegativeInt" />
        <xs:element name="placed" type="EnhancedCallLogsEntries" minOccurs="0" />
        <xs:element name="received" type="EnhancedCallLogsEntries" minOccurs="0" />
        <xs:element name="missed" type="EnhancedCallLogsEntries" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="Enterprise">
    <xs:annotation>
      <xs:documentation>
        This service allows the retrieval of a user’s enterprise directory.
       
        This command supports paging of responses.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="startIndex" type="PositiveInt">
          <xs:annotation>
            <xs:documentation>
              The startIndex attribute refers to the position/pointer from 
              which the results will be given.
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="numberOfRecords" type="NonNegativeInt">
          <xs:annotation>
            <xs:documentation>
              The numberOfRecords attribute refers to the number of results 
              returned in the response. The maximum value of number of 
              records will not be greater than the page size.
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="totalAvailableRecords" type="NonNegativeInt">
          <xs:annotation>
            <xs:documentation>
              The totalAvailableRecords attribute refers to the total number
              of responses found for the query.
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="enterpriseDirectory" type="UserAdditionalDetailsList" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="Group">
    <xs:annotation>
      <xs:documentation>
        This service allows the retrieval of a user’s group directory.
        
        This command supports paging of responses.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="startIndex" type="PositiveInt">
          <xs:annotation>
            <xs:documentation>
              The startIndex attribute refers to the position/pointer from 
              which the results will be given.
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="numberOfRecords" type="NonNegativeInt">
          <xs:annotation>
            <xs:documentation>
              The numberOfRecords attribute refers to the number of results 
              returned in the response. The maximum value of number of 
              records will not be greater than the page size.
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="totalAvailableRecords" type="NonNegativeInt">
          <xs:annotation>
            <xs:documentation>
              The totalAvailableRecords attribute refers to the total number
              of responses found for the query.
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="groupDirectory" type="UserAdditionalDetailsList" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="Personal">
    <xs:annotation>
      <xs:documentation>
        The personal directory request retrieves all the personal 
        contacts of a user, sorted in alphabetical order by name.

        This command supports paging of responses.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="startIndex" type="PositiveInt" minOccurs="0">
          <xs:annotation>
            <xs:documentation>
              The startIndex attribute refers to the position/pointer from 
              which the results will be given.
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="numberOfRecords" type="NonNegativeInt" minOccurs="0">
          <xs:annotation>
            <xs:documentation>
              The numberOfRecords attribute refers to the number of results 
              returned in the response. The maximum value of number of 
              records will not be greater than the page size.
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="totalAvailableRecords" type="NonNegativeInt" minOccurs="0">
          <xs:annotation>
            <xs:documentation>
              The totalAvailableRecords attribute refers to the total number
              of responses found for the query.
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="entry" type="PersonalEntry" minOccurs="0" maxOccurs="unbounded" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:complexType name="ContactDetails">
    <xs:annotation>
      <xs:documentation>
        This is the Agent Profile for a particular directory entry.
        
        Release Modified: 18.0
        Added IMPUserId.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="UserDetails">
        <xs:sequence>
          <xs:element name="name" type="LastFirstName" minOccurs="0" />
          <xs:element name="mobile" type="DN" minOccurs="0" />
          <xs:element name="title" type="Title" minOccurs="0" />
          <xs:element name="emailAddress" type="EmailAddress" minOccurs="0" />
          <xs:element name="department" type="DepartmentName" minOccurs="0" />
          <xs:element name="addressOfRecord" type="AddressOfRecord" minOccurs="0">
            <xs:annotation>
              <xs:documentation>
                In standalone mode, the address of record identifies a
                Line/Port for a device profile. In IMS mode, the address 
                of record identifies a public identity for an identity 
                profile.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="impId" type="IMPUserId" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="CallLogsEntries">
    <xs:annotation>
      <xs:documentation>
        Call Log entries describing single/multiple placed or received 
        or missed calls.
        
        This type supports paging.  
   </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="startIndex" type="PositiveInt" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The startIndex attribute refers to the position/pointer from 
            which the results will be given.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="numberOfRecords" type="NonNegativeInt" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The numberOfRecords attribute refers to the number of results 
            returned in the response. The maximum value of number of 
            records will not be greater than the page size.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="callLogsEntry" type="CallLogsEntry" minOccurs="0"
        maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  
  <xs:complexType name="EnhancedCallLogsEntries">
    <xs:annotation>
      <xs:documentation>
        Enhanced Call Log entries describing single/multiple placed 
        or received or missed calls. 
        
        This type supports paging.  
        
        Release Modified: 17sp4. 
        In this release the enhanced call log entry can be an an 
        extended entry with more information. The entry is a now a 
        choice bewteen regular enhanced call log entry or enhanced 
        call log extended entry.
   </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="startIndex" type="PositiveInt" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The startIndex attribute refers to the position/pointer from 
            which the results will be given.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="numberOfRecords" type="NonNegativeInt" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            The numberOfRecords attribute refers to the number of results 
            returned in the response. The maximum value of number of 
            records will not be greater than the page size.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:choice>
        <xs:element name="enhancedCallLogsEntry" type="EnhancedCallLogsEntry"
          minOccurs="0" maxOccurs="unbounded" />
        <xs:element name="enhancedCallLogsExtendedEntry"
          type="EnhancedCallLogsExtendedEntry" minOccurs="0" maxOccurs="unbounded" />
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  

  <xs:complexType name="CallLogsEntry">
    <xs:annotation>
      <xs:documentation>
        Call Log entry describing a placed, received, or
        missed call.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="countryCode" type="CountryCode" minOccurs="0" />
      <xs:element name="phoneNumber" type="OutgoingDNorSIPURI" />
      <xs:element name="name" type="CallLogsEntryName" />
      <xs:element name="time" type="xs:dateTime" />
      <xs:element name="callLogId" type="CallLogId"/>      
    </xs:sequence>
  </xs:complexType>
  
  <xs:complexType name="EnhancedCallLogsEntry">
    <xs:annotation>
      <xs:documentation>
        Enhanced Call Log entry describing a placed, received, or
        missed call.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="countryCode" type="CountryCode" minOccurs="0" />
      <xs:element name="phoneNumber" type="OutgoingDNorSIPURI" />
      <xs:element name="name" type="CallLogsEntryName" />
      <xs:element name="time" type="xs:dateTime" />
      <xs:element name="callLogId" type="CallLogId"/>            
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="PersonalEntry">
    <xs:annotation>
      <xs:documentation>
        Phone List Entry Details.
        
        New personal directories can be created using this service.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="name" type="PhoneListEntryName" />
      <xs:element name="number" type="DN" />
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="CallLogsEntryName">
    <xs:annotation>
      <xs:documentation>
        The calling/called party name as saved when a user
        received/placed a call.
   </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1" />
      <xs:maxLength value="62" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="CustomContactDirectoryName">
    <xs:annotation>
      <xs:documentation>
        Custom Contact Directory name.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1" />
      <xs:maxLength value="40" />
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="EnterpriseCommon">
    <xs:annotation>
      <xs:documentation>
        This service allows the retrieval of a user’s enterprise common phone 
        list for a user.
        
        Change History:
        R20.0 - Added startIndex, numberOfRecords, and totalAvailableRecords
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
      <xs:element name="startIndex" type="PositiveInt">
          <xs:annotation>
            <xs:documentation>
              The startIndex attribute refers to the position/pointer from 
              which the results will be given.
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="numberOfRecords" type="NonNegativeInt">
          <xs:annotation>
            <xs:documentation>
              The numberOfRecords attribute refers to the number of results 
              returned in the response. The maximum value of number of 
              records will not be greater than the page size.
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="totalAvailableRecords" type="NonNegativeInt">
          <xs:annotation>
            <xs:documentation>
              The totalAvailableRecords attribute refers to the total number
              of responses found for the query.
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="commonPhoneEntry" type="PersonalEntry"
          minOccurs="0" maxOccurs="unbounded" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  
  <xs:element name="GroupCommon">
    <xs:annotation>
      <xs:documentation>
        This service allows the retrieval of a user’s group common phone list.
        
        Change History:
        R20.0 - Added startIndex, numberOfRecords, and totalAvailableRecords
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
       <xs:sequence>
    		<xs:element name="startIndex" type="PositiveInt">
          <xs:annotation>
            <xs:documentation>
              The startIndex attribute refers to the position/pointer from 
              which the results will be given.
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="numberOfRecords" type="NonNegativeInt">
          <xs:annotation>
            <xs:documentation>
              The numberOfRecords attribute refers to the number of results 
              returned in the response. The maximum value of number of 
              records will not be greater than the page size.
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="totalAvailableRecords" type="NonNegativeInt">
          <xs:annotation>
            <xs:documentation>
              The totalAvailableRecords attribute refers to the total number
              of responses found for the query.
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="commonPhoneEntry" type="PersonalEntry"
          minOccurs="0" maxOccurs="unbounded" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:simpleType name="CallingPresentationNumberSource">
    <xs:annotation>
      <xs:documentation>
        Identifies the source of the number used in the Calling 
        Presentation Number field.
  
        Change History:
        R17.sp4 - Added
        R18 - Added Department
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="Configurable CLID" />
      <xs:enumeration value="DNIS" />
      <xs:enumeration value="Emergency" />
      <xs:enumeration value="Group" />
      <xs:enumeration value="Trunk Group" />
      <xs:enumeration value="User" />
      <xs:enumeration value="Department" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="ConnectedNumberSource">
    <xs:annotation>
      <xs:documentation>
        The Source for the call provided in the most significant 
        contact from the NS.

        Change History:
        R17.sp4 - Added
        R18 - Added Department
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
